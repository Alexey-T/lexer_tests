// with declared type
var a: bool = false || false; // == false
var b: bool = false || true;  // == true
var c: bool = true || false;  // == true
var d: bool = true || true;   // == true
// with inferred type
var e = false || false; // == false
var f = false || true;  // == true
var g = true || false;  // == true
var h = true || true;   // == true
