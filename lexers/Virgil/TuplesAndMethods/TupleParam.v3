def main() {
	var t = (3, 4);
	// both individual parameters and a single tuple are equivalent
	m1(3, 4);
	m1(t);
	// both individual parameters and a single tuple are equivalent
	m2(3, 4);
	m2(t);
}
def m1(a: int, b: int) -> int {
	return a + b;
}
def m2(t: (int, int)) -> int {
	return t.0 + t.1;
}
