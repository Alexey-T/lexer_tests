def main(a: Array<string>) {
	var f: (int, int) -> int;
	if (a.length > 0) f = m1;
	else f = m2;
	f(0, 2); // invocation of first-class function
}
def m1(a: int, b: int) -> int {
	System.puti(a);
	System.puts("\n");
	return a + b;
}
def m2(a: int, b: int) -> int {
	System.puti(b);
	System.puts("\n");
	return a + b;
}
