class Animal {
	// an example class to illustrate casts with objects
}
class Mammal extends Animal {
	// an example class to illustrate casts with objects
}
def main() {
	var x = Animal.new();
	var y = Mammal.new();
	for (e in [x, y]) {
		// dynamically query the type of each object
		if (Mammal.?(e)) System.puts("Mammal");
		else System.puts("Animal");
		System.puts("\n");
	}
}
