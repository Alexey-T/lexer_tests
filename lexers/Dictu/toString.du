/**
 * toString.du
 *
 * Testing the nil.toString() method
 *
 * .toString() returns the string representation of nil
 */

from UnitTest import UnitTest;

class NilToStringTest < UnitTest {
    testNilToString() {
        this.assertTruthy(nil.toString() == "nil");
    }

    testNilVariableToString() {
        const testnil = nil;
        this.assertTruthy(testnil.toString() == "nil");
    }
}

NilToStringTest().run();
